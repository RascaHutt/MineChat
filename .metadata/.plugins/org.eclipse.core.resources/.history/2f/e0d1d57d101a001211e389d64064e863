name: MineChat
main: tk.mhutti1.minechat.MineChat
version: 1
commands:
 getverifcode:
  description: verifies user
  usage: /<command> <email>
  permission: <plugin name>.special
  permission-message: You don't have <permission>
 webchat:
  description: chats to web
  usage: /<command> <text>
  permission: MineChat.special
  permission-message: You don't have <permission>
permissions:
 special:
  default: true